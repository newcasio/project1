<%= form_for @conflict, html: {multipart: true} do |f| %>   <!--html part for cloudinary -->

  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </fieldset>
  <fieldset>
    <%= f.label :country %>
    <%= f.text_field :country %>
  </fieldset>
  <fieldset>
    <%= f.label :conflict_type %>
    <%= f.text_field :conflict_type %>
  </fieldset>
  <fieldset>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </fieldset>
  <fieldset>
    <%= f.label :parties %>
    <%= f.text_field :parties %>
  </fieldset>
  <fieldset>
    <%= f.label :latitude %>
    <%= f.text_field :lat %>
  </fieldset>
  <fieldset>
    <%= f.label :longitude %>
    <%= f.text_field :lng%>
  </fieldset>
  <fieldset>
    <%= f.label :continent_id, 'Continent' %>  <!--drop down selection-->
    <%= f.collection_select :continent_id, Continent.all, :id, :name, prompt: 'Please choose a continent'  %>
  </fieldset>

  <fieldset>
    <%= f.label :image, 'Image URL' %>
    <%= f.text_field :image %>            <!--cloudinary change -->
  </fieldset>
  <%= f.submit %>

<% end %>
